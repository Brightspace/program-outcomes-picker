  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "dependencies": {
    "@brightspace-ui/core": "^1.43.2",
    "d2l-alert": "github:BrightspaceUI/alert#semver:^4",
    "lit-element": "^2.2.1"
  },
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "babel-eslint": "^10.0.3",
    "d2l-polymer-langtools": "^1.4.0",
    "es-dev-server": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-brightspace": "^0.6.4",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-lit": "^1.2.0",
    "eslint-plugin-sort-class-members": "^1.6.0"
  }
}
