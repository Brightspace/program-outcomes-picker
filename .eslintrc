{
	"extends": "brightspace/lit-config",
	"rules": {
		"no-unused-vars": 2,
		"no-console": 1,
		"keyword-spacing": 0,
		"lit/no-private-properties": 0,
		"sort-imports": 0,
		"space-in-parens": 0,
		"no-trailing-spaces": 0,
		"no-warning-comments": 1,
		"prefer-template": 0
	},
	"parserOptions": {
		"ecmaVersion": 2017,
		"sourceType": "module"
	},
	"globals": {
		"Set": true,
		"Map": true,
		"Promise": true
	}
}
